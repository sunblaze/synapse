<h1>Listing atomic_literals</h1>

<table>
  <tr>
    <th>Primary expr</th>
  </tr>

<% for atomic_literal in @atomic_literals %>
  <tr>
    <td><%=h atomic_literal.primary_expr %></td>
    <td><%= link_to 'Show', atomic_literal %></td>
    <td><%= link_to 'Edit', edit_atomic_literal_path(atomic_literal) %></td>
    <td><%= link_to 'Destroy', atomic_literal, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New atomic_literal', new_atomic_literal_path %>
